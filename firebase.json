{
  "hosting": {
    "public": "_site",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=600"
          }
        ]
      },
      {
        "source": "generated/assets/img/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/blogging/2021/02/19/new-desk-part-1-buying-a-frame.html",
        "destination": "/blogging/2021/02/20/new-desk-part-1-buying-a-frame.html",
        "type": 301
      },
      {
        "source": "/blogging/2021/02/19/new-desk-part-2-finding-a-slab.html",
        "destination": "/blogging/2021/02/20/new-desk-part-2-finding-a-slab.html",
        "type": 301
      },
      {
        "source": "/blogging/2021/02/19/new-desk-part-3-gap-filling-and-sanding.html",
